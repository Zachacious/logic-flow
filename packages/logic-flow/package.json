{
  "name": "logic-flow",
  "version": "0.0.1",
  "description": "Framework agnostic web components for building non-linear user interfaces, workflow editors, logic flows and more.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/logic-flow/logic-flow.esm.js",
  "exports": {
    ".": {
      "import": "./dist/logic-flow/logic-flow.esm.js",
      "require": "./dist/logic-flow/logic-flow.cjs.js"
    },
    "./my-component": {
      "import": "./dist/components/my-component.js",
      "types": "./dist/components/my-component.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^4.21.0",
    "@types/jest": "^29.5.13",
    "@types/node": "^16.18.112",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^21.11.0"
  },
  "license": "MIT",
  "dependencies": {
    "@stencil/vue-output-target": "^0.8.9",
    "add": "^2.0.6",
    "bun": "^1.1.29",
    "interactjs": "^1.10.27",
    "nanoid": "^5.0.7",
    "tseep": "^1.3.1"
  }
}
